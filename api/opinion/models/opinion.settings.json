{
  "connection": "default",
  "collectionName": "opinions",
  "info": {
    "name": "opinion"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "description": {
      "type": "string"
    },
    "score": {
      "type": "integer",
      "default": 1,
      "max": 5,
      "min": 1
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
